 /*
 -----------------------------------------------------------

					       Version: 1.9

-----------------------------------------------------------

   							Colours

-----------------------------------------------------------

		Backgrounds			#1c1c1c /*Background*//*

		Text				#ddd /*Text*//*
		Text Hover Button	#000000 /*Text1*//* (Make this opposite to text)

		Borders				#3c3c3c /*Border*//*

		Accents - Green		#a1e02d /*Accent*//*
		Accents - Red		#ed4c37 /*Accent*//*
		Accents - Blue		#2d9be0 /*Accent*//*

		Styles Pack
		Green				SimplexGreenWhite
		Red					SimplexRedWhite
		Blue				SimplexBlueWhite

-----------------------------------------------------------
*/

QWidget {
	background-color: transparent;
	color: #ddd;
}

QToolTip {
	background-color: #1c1c1c;
	border: 1px solid #3c3c3c;
	color: #ddd;
}

QAbstractItemView {
	background-color: #1c1c1c; /*Background*/
}

QDialog {
	border: 10px solid #1c1c1c; /*Background*/
	border-radius: 0px;
	background-color: #1c1c1c; /*Background*/
	color: aqua;
	/*text-transform: uppercase;*/
	/*font-weight: bold;*/
	padding: 19px 15px;
}

QFrame {
	background-color: transparent;
	color: #ddd; /*Text*/
	/*text-transform: normal;*/
}
QFrame:hover{
	color: #ed4c37; /*Accent*/
}

QGroupBox{
	background-color: transparent;
	border: 1px solid #3c3c3c; /*Border*/
	color: #ed4c37; /*Accent*/
	margin-top: 15px;
	padding-top: 10px;
}

QMainWindow	{
	background-repeat: no-repeat;
	background-position: center top;
 	background-color: #1c1c1c; /*Background*/
	/*text-transform: normal;*/
	color: #ffffff;
}

QWidget#MainWindowServerTabsWidget {
	border: 0px solid transparent;
	background-color: transparent;
	background-clip: padding;
	show-decoration-selected: 0;
	margin-left: 30px;
	margin-right: 30px;
	margin-top: 75px;
}

QWidget::pane {
	color: transparent;
	background: transparent;
	border: 0px solid transparent;
    padding: 0;
    margin: 0;
}

QFrame#bookmarksTopFrame {
	background: transparent;
}


/* ----------------------------------------------------------------	*/
/* 			 Chat Area || ChatWidget, ChatArea and ChatLine			*/
/* ---------------------------------------------------------------- */

QWidget#MainWindowChatWidget {
	border: 0px transparent;
	background-color: transparent;
	show-decoration-selected: 0;
}

QLineEdit {
	border: 1px solid #3c3c3c; /*Border*/
	height: 21px;
	background-color: #1c1c1c; /*Background*/
	color: #ddd; /*Text*/
}

ChatLineEdit {
	border: 1px solid #3c3c3c; /*Border*/
	height: 21px;
	background-color: #1c1c1c; /*Background*/
	color: #ddd; /*Text*/
}

ChatTab {
	border: 1px solid #3c3c3c; /*Border*/
	background-color: #1c1c1c; /*Background*/
	/*text-transform: normal;*/
	font-family: Helvetica, Arial, sans-serif;
	color: #ddd; /*Text*/
	padding: 4px;
	show-decoration-selected: 0;
}

QTextEdit#pluginDescTextEdit,		/* Description Box in the Plugin window */
QTextEdit#descriptionTextEdit,			/**/
QTextEdit#vserverWelcomeTextEdit, 		/* Changelog text box */
QTextEdit#changeLogTextEdit,
QLineEdit:read-only {
	border: 1px solid #3c3c3c; /*Accent*/
	background-color: transparent;
	color: #ddd; /*Text*/
	padding: 6px;
}

/* ----------------------------------------------------------------	*/
/* 							InfoFrame | InfoView					*/
/* ---------------------------------------------------------------- */

InfoFrame {
	background-color: #1c1c1c; /*Background*/
	border: 1px solid #3c3c3c; /*Border*/
	color: #ddd; /*Text*/
	font-size: 12px;
	font-family: Arial, Helvetica, Verdana;
	padding: 4px;
}


/* ----------------------------------------------------------------	*/
/* 								TreeView 							*/
/* ---------------------------------------------------------------- */


QTreeView,
QTreeWidget {
	padding: 4px;
	border: 1px solid #3c3c3c;
	background-color: #1c1c1c; /*Background*/
	background-attachment: fixed;
	show-decoration-selected: 0;
	/*text-transform: normal;*/
}

TreeView,
TreeWidget {
	background-color: #1c1c1c; /*Background*/
	/*text-transform: normal;*/
	color: #ddd; /*Text*/
	qproperty-alternatingRowColors: 0;
}

QTreeView::item {
	/*text-transform: normal;*/
}
QTreeView::item:hover,
QTreeView::item:selected:active {
	background-color: #1c1c1c; /*Background*/
	color: #ed4c37; /*Accent*/
}
QTreeView::branch:hover {
	background-color: #1c1c1c; /*Background*/
	color: #ed4c37; /*Accent*/
}
QTreeView::item:selected:!active {
	background-color: #1c1c1c; /*Background*/
	color: #ed4c37; /*Accent*/
}


/* ----------------------------------------------------------------	*/
/* 								Tree Branch							*/
/* ---------------------------------------------------------------- */

QTreeView::branch,
QTreeView::branch:selected,
QTreeView::branch:selected:!active{	background-color: #1c1c1c; /*Background*/	}


QTreeView::branch:has-siblings:adjoins-item,
QTreeView::branch:!has-children:!has-siblings:adjoins-item {
	background-color: #1c1c1c; /*Background*/
	border-image: none;
}

QTreeView::branch:has-siblings:adjoins-item:selected,
QTreeView::branch:!has-children:!has-siblings:adjoins-item:selected {
	background-color: #1c1c1c; /*Background*/
	border-image: none;
}

QTreeView::branch:has-siblings:adjoins-item:selected:!active,
QTreeView::branch:!has-children:!has-siblings:adjoins-item:selected:!active {
	background-color: #1c1c1c; /*Background*/
	border-image: none;

}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
	border-image: none;
	background-color: #1c1c1c; /*Background*/
	image: url('styles:DarkStyle/misc/treeview/tree-collapse-normal.png');
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings {
	border-image: none;
	background-color: #1c1c1c; /*Background*/
	image: url('styles:DarkStyle/misc/treeview/tree-expand-normal.png');
}

QTreeView::branch:has-children:!has-siblings:closed:selected,
QTreeView::branch:closed:has-children:has-siblings:selected {
	border-image: none;
	background-color: #1c1c1c; /*Background*/

}

QTreeView::branch:open:has-children:!has-siblings:selected,
QTreeView::branch:open:has-children:has-siblings:selected  {
	border-image: none;
	background-color: #1c1c1c; /*Background*/
}

QTreeView::branch:has-children:!has-siblings:closed:hover,
QTreeView::branch:closed:has-children:has-siblings:hover {
	border-image: none;
	background-color: #1c1c1c; /*Background*/
	image: url('styles:DarkStyle/misc/treeview/tree-collapse-normal-hover.png');

}

QTreeView::branch:open:has-children:!has-siblings:hover,
QTreeView::branch:open:has-children:has-siblings:hover  {
	border-image: none;
	background-color: #1c1c1c; /*Background*/
	image: url('styles:DarkStyle/misc/treeview/tree-expand-normal-hover.png');
}

QTreeView::branch:has-children:!has-siblings:closed:selected:!active,
QTreeView::branch:closed:has-children:has-siblings:selected:!active {
	border-image: none;
	background-color: #1c1c1c; /*Background*/

}

QTreeView::branch:open:has-children:!has-siblings:selected:!active,
QTreeView::branch:open:has-children:has-siblings:selected:!active {
	border-image: none;
	background-color: #1c1c1c; /*Background*/
}


/* ----------------------------------------------------------------	*/
/* 				QLabel | Header text for options window				*/
/* ---------------------------------------------------------------- */

QWidget#HeadlineLabel {
