/* ----------------------------------------------------------------	*/
/* 								QTabBar								*/
/* ---------------------------------------------------------------- */

/*QTabWidget#permTabWidget::pane 	{	font-family: segoeuil_0;}*/
QTabWidget::tab-bar 			{}
QTabWidget::tab-bar:bottom		{}
QTabWidget::tab-bar:top 		{}

QTabWidget::pane {}


QTabBar 				{}
QTabBar::pane			{}
QTabBar:hover 			{}
QTabBar:top				{}
QTabBar::tab:top:only-one	{}
QTabBar::tab:top:first  {}

QTabBar::tab {
	padding: 4px;
	padding-left: 10px;
    padding-right: 8px;
	background-color: #1c1c1c; /*Background*/
	color: #ddd; /*Text*/
	font-size: 12px;
	height: 22px;
	min-width: 100px;
	/*border: none;*/
	border-right: 1px solid #3c3c3c;
}
QTabBar::tab:first {
	border-left: 1px solid #3c3c3c;
}
QTabBar::tab:only-one {
	border-right: 1px solid #3c3c3c;
	border-left: 1px solid #3c3c3c;
}
QTabBar::tab:top {
	border-top: 1px solid #3c3c3c;
}
QTabBar::tab:bottom {
	border-bottom: 1px solid #3c3c3c;
}

QTabBar::tab:bottom:selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 #1c1c1c,
		stop: 0.9 #1c1c1c,
		stop: 0.901 #ed4c37,
		stop: 1.0 #ed4c37);
	border-bottom: 1px solid #ed4c37;
}
QTabBar::tab:top:selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 #ed4c37,
		stop: 0.10 #ed4c37,
		stop: 0.101 #1c1c1c,
		stop: 1.0 #1c1c1c);
	border-top: none;
	height: 23px;
}

QTabBar::tab:bottom:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 #1c1c1c,
		stop: 0.93 #1c1c1c,
		stop: 0.94 #ed4c37,
		stop: 1.0 #ed4c37);
	border-bottom: none;
	height: 23px;
}
QTabBar::tab:top:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 #ed4c37,
		stop: 0.05 #ed4c37,
		stop: 0.06 #1c1c1c,
		stop: 1.0 #1c1c1c);
	border-top: none;
	height: 22px;
}

