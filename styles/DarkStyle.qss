/* ----------------------------------------------------------------	*/
/* 						Pushbutton | Ok, Cancel, etc				*/
/* ---------------------------------------------------------------- */

QPushButton {
    border: 0px solid transparent;
	padding-left: 10px;
	padding-right: 10px;
	padding-bottom: 6px;
	padding-top: 6px;

	background-color: #3c3c3c; /*Border*/
	height: 16px;
	min-width: 70px;
	color: #ddd; /*Text*/
	font-size: 12px;
	text-align: center;
}

QPushButton:default {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 #3c3c3c,
		stop: 0.93 #3c3c3c,
		stop: 0.94 #ed4c37,
		stop: 1.0 #ed4c37);
}

QPushButton:hover {
    /*border: 0px solid transparent;*/
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 #333,
		stop: 0.97 #333,
		stop: 0.98 #ed4c37,
		stop: 1.0 #ed4c37);
	/*padding-left: 10px;
	padding-right: 10px;
	padding-bottom: 6px;
	padding-top: 6px;*/
}

QPushButton:pressed {
	background-color: #333;
	color: #999;
}

QPushButton:disabled {
	color: #808080;
}


/* ----------------------------------------------------------------	*/
/* 			QMenuBar | Top menu (Connections, Bookmarks)			*/
/* ---------------------------------------------------------------- */

QMenuBar {
	min-height: 22px;
  	background-color: #1c1c1c; /*Background*/
	font-size: 12px;
	color: #ddd; /*Text*/
}

QMenuBar::item {
	background-color: #1c1c1c; /*Background*/
	color: #ddd; /*Text*/
}

QMenuBar::item:selected {
	color: #ed4c37; /*Accent*/
}

QMenuBar::item:hover {
	color: #ed4c37; /*Accent*/
}

/* ----------------------------------------------------------------	*/
/* 							QMenu | Right Click						*/
/* ---------------------------------------------------------------- */
QMenu {
	background-color: #1c1c1c; /*Background*/
	border: 1px solid #3c3c3c;
	font-size: 12px;
	color: #ddd; /*Text*/
	padding-top: 0px;
	padding-left: 0px;
	padding: 4px;
}
QMenu::item {
	background: #1c1c1c; /*Background*/
	padding-top: 4px;
	padding-bottom: 4px;
	padding-right: 4px;
	padding-left: 23px;
}

QMenu::icon { padding-left: 3px;}

QMenu::item:selected {
	background-color: transparent;
	color: #ed4c37; /*Accent*/
}
QMenu::item:disabled {
	color: #808080;
}

QMenu::separator {
	height:1px;
	margin-left: 0px;
	margin-right: 0px;
	background: #3c3c3c;
}
QMenu::indicator:non-exclusive:checked {
	background-image:url('styles:DarkStyle/misc/tick.png')
}


/* ----------------------------------------------------------------	*/
/* 				 ListView | Options list, Profiles list				*/
/* ---------------------------------------------------------------- */

QListView {
	border-top: 20px transparent;
	border-bottom: 20px transparent;
	border-right: 20px transparent;
	border-left: 20px transparent;
	background-color: transparent;
	min-width: 140px;
	color: #ddd; /*Text*/
	font-weight: normal;
	selection-color: #ed4c37; /*Accent*/
	selection-background-color: #1c1c1c; /*Background*/
}

QListView::item:hover,
QListView::item:selected:active,
QListView::item:selected:!active {
	background-color: transparent;
	color: #ed4c37; /*Accent*/
}

QListWidget#identitiesListWidget,
QListView#fileListView::item:hover {
	background-color: #1c1c1c; /*Background*/
}
QListView#fileListView::item:selected {
	background-color: #e1e1e1;
}


/* ----------------------------------------------------------------	*/
/* 						 ComboBox | Dropdown Menu					*/
/* ---------------------------------------------------------------- */

QComboBox {
	background-color: #1c1c1c;
	border: 1px solid #3c3c3c;
	color: #ffffff;
	min-width: 140px;
	height: 27px;
	text-align: left;
	padding-left: 5px;
	padding-right: 10px;
	selection-background-color: #ed4c37;
}

QComboBox:hover {
	color: #ed4c37;
}

QComboBox:!editable:on,
QComboBox::drop-down:editable:on {
	background-color: #1c1c1c;
}
QComboBox:on {
	background-color: #1c1c1c;
}
QComboBox::down-arrow {
	image: url('styles:DarkStyle/dialog/combobox.png');
}

QComboBox#utcLocalComboBox {
	padding-left: 14px;
	padding-right: 7px;
}

QComboBox::drop-down {
	background-color: transparent;
	border-left: 1px solid #3c3c3c;
	border-image: none;
	subcontrol-origin: padding;
	subcontrol-position: top right;
	color: #d1cfcd;
	width: 24px;
}

QComboBox QAbstractItemView {
	background-color: #1c1c1c;
	border: 1px solid #3c3c3c;
	border-image: none;
	color: #ffffff;
}
QComboBox QAbstractItemView:hover {
	color: #ed4c37;
}


/* ----------------------------------------------------------------	*/
/* 					 SpinBox | Scrolling Numbers Box				*/
/* ---------------------------------------------------------------- */

QSpinBox,
QDoubleSpinBox#delayPTTSpinBox {
	background-color: #1c1c1c; /*Background*/
	border: 1px solid #3c3c3c; /*Border*/
	color: #ddd; /*Text*/
	min-width: 80px;
	height: 21px;
	text-align: left;
	padding-left: 5px;
	padding-right: 10px;
	padding-top: 2px;
	padding-bottom: 2px;
	selection-background-color: none;
}

QSpinBox::up-button {
    width: 24px;
    background-image: none;
	background-repeat: no-repeat;
	background-position: center center;
	border-left: 1px solid #3c3c3c; /*Border*/
	background-color: transparent;
}

QSpinBox::up-arrow {
     background-image: url('styles:DarkStyle/dialog/spinner-up.png');
	 background-position: center center;
	 background-repeat: no-repeat;
}

QSpinBox::down-button {
    width: 24px;
    background-image: none;
	background-repeat: no-repeat;
	background-position: center center;
	border-left: 1px solid #3c3c3c; /*Border*/
	background-color: transparent;
}

 QSpinBox::down-arrow {
     background-image: url('styles:DarkStyle/dialog/spinner-down.png');
	 background-position: center center;
	 background-repeat: no-repeat;
 }


QSpinBox::up-arrow:disabled,
QSpinBox::up-arrow:off 			{background: transparent;}
QSpinBox::down-arrow:disabled,
QSpinBox::down-arrow:off 		{background: transparent;}


/* ----------------------------------------------------------------	*/
/* 						ScrollBar | Vertical						*/
/* ---------------------------------------------------------------- */

QScrollBar:vertical {
	/*background-color: #3c3c3c;*/ /*Border*/
	background-color: transparent;
	width: 10px;
	margin: 15px 0px 15px 0;
}

QScrollBar::handle:vertical {
	border: 0px solid transparent;
	background-color: #3c3c3c; /*Accent*/
	margin-top: 0px;
	margin-bottom: 0px;
}
QScrollBar::handle:vertical:hover {
	border: 0px solid transparent;
	background-color: #3c3c3c; /*Accent*/
	margin-top: 0px;
	margin-bottom: 0px;
}
QScrollBar::add-line:vertical {
	border-radius: 3px;
	border: 0px solid transparent;
	background-color: transparent;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
	height: 8px;
}
QScrollBar::sub-line:vertical {
	border-radius: 3px;
	border: 0px solid transparent;
	background-color: transparent;
	subcontrol-position: top;
	subcontrol-origin: margin;
	height: 8px;
}

QScrollBar::up-arrow:vertical {
	image: url('styles:DarkStyle/dialog/spinner-up.png');
	background: none;
	background-repeat: none;
}
QScrollBar::down-arrow:vertical {
	image: url('styles:DarkStyle/dialog/spinner-down.png');
	background: none;
	background-repeat: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}


/* ----------------------------------------------------------------	*/
/* 						ScrollBar | Horizontal						*/
/* ---------------------------------------------------------------- */

QScrollBar:horizontal {
	/* border: 0px solid transparent; */
	/*background-color: #3c3c3c;*/ /*Border*/
	background-color: transparent;
	height: 10px;
	margin: 0px 15px 0px 15px;
}
QScrollBar::handle:horizontal {
	border: 0px solid transparent;
	background-color: #3c3c3c; /*Accent*/
	margin-top: 0px;
	margin-bottom: 0px;
}
QScrollBar::handle:horizontal:hover {
	border: 0px solid transparent;
	background-color: #3c3c3c; /*Accent*/
	margin-top: 0px;
	margin-bottom: 0px;
}
QScrollBar::add-line:horizontal  {
	border-radius: 3px;
	border: 0px solid transparent;
	background-color: transparent;
	subcontrol-position: right;
	subcontrol-origin: margin;
	width: 8px;
}
QScrollBar::sub-line:horizontal {
	border-radius: 3px;
	border: 0px solid transparent;
	background-color: transparent;
	subcontrol-position: left;
	subcontrol-origin: margin;
	width: 8px;
}

QScrollBar::left-arrow:horizontal {
	image: url('styles:DarkStyle/dialog/spinner-left.png');
	background: none;
	background-repeat: none;
}
QScrollBar::right-arrow:horizontal {
	image: url('styles:DarkStyle/dialog/spinner-right.png');
	background: none;
	background-repeat: none;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}


/* ----------------------------------------------------------------	*/
/* 			TableView	|	Hotkeys, Messages, File Browser			*/
/* ---------------------------------------------------------------- */


QHeaderView::section {
	background-color: #1c1c1c; /*Background*/
	border: 1px solid #3c3c3c; /*Border*/
	color: #ddd; /*Text*/
	font-weight: bold;
	height: 23px;
	padding: 2px;
}

QTableView {
	background-color: transparent;
	border: 0px solid transparent;
	border-radius: 0px;
	color: #ddd; /*Text*/
	selection-color: #ed4c37; /*Accent*/
	font-weight: normal;
	selection-background-color: #1c1c1c; /*Background*/
	gridline-color: #3c3c3c; /*Border*/
	alternate-background-color: transparent;
}

QTableView::item {
	border: 0px solid transparent;
}
QTableView::item:hover {
	background-color: #1c1c1c; /*Background*/
	color: #ed4c37; /*Accent*/
}

/* ----------------------------------------------------------------	*/
/* 							QSlider	 | Volume Control				*/
/* ---------------------------------------------------------------- */

QSlider:horizontal	{	margin-right: 20px;	}
QSlider:vertical	{	margin-right: 0px;
}

QSlider::groove:horizontal {
	border: 0px solid;
	height: 2px;
	background-color: #333;
	margin: 9px 0;
}

QSlider::handle:horizontal {
	background-color: #ed4c37; /*Accent*/
	border: 0px;
	/* border-radius: 2px; */
	width: 6px;
	height: 16px;
	margin: -16px 0;
	border-radius: 0px;
}

QSlider::groove:vertical {
	border: 0px solid;
	width: 2px;
	background-color: #333;
	margin: 0 2px;
}

QSlider::handle:vertical {
	background-color: #ed4c37; /*Accent*/
	border: 0px solid;
	height: 4px;
	width: 10px;
	margin: 0 -10px;
	border-radius: 0px;
}


/* ----------------------------------------------------------------	*/
/* 							Misc | CheckBox, Radiobox				*/
/* ---------------------------------------------------------------- */

QRadioButton	{ color: #ffffff; }
QRadioButton:hover	{ color: #ed4c37; }

QCheckBox {
	background-color: transparent;
    spacing: 8px;
	margin-top: 5px;
	color: #ffffff;
}

QCheckBox:hover {
	color: #ed4c37;
}

QCheckBox::indicator {
    width: 18px;
    height: 18px;
	border: 0px solid transparent;
	background-color: transparent;
	background-image:url('styles:DarkStyle/misc/checkbox-normal.png');
}

QCheckBox::indicator:unchecked {
	background-color: transparent;
}
QCheckBox::indicator:unchecked:pressed {}

QCheckBox::indicator:checked {
    background-image:url('styles:DarkStyle/misc/checkbox-checked.png');
	background-position: center;
	background-repeat:no-repeat;
	background-color: transparent;
}

QCheckBox::indicator:checked:hover{
    background-image:url('styles:DarkStyle/misc/checkbox-checked.png');
	background-position: center;
	background-repeat:no-repeat;
	background-color: transparent;
}

QCheckBox::indicator:checked:pressed {
	background-image:url('styles:DarkStyle/misc/checkbox-checked.png');
	background-position: center;
	background-repeat:no-repeat;
	background-color: transparent;
}

QCheckBox::indicator:indeterminate:hover {
	background: transparent;
	border: 1px solid #808080;
	border-radius: 5px;
}

QCheckBox::indicator:indeterminate:pressed {
	background: transparent;
	border: 1px solid #808080;
	border-radius: 5px;
}


QRadioButton::indicator {
	width: 18px;
	height: 18px;
	margin-top: 5px;
	Background-image:url('styles:DarkStyle/misc/radiobutton.png');
}

 QRadioButton::indicator:unchecked:pressed {
	background-image:url('styles:DarkStyle/misc/radiobutton-checked.png');
 }
 QRadioButton::indicator::checked {
	background-image:url('styles:DarkStyle/misc/radiobutton-checked.png');
}
 QRadioButton::indicator:checked:hover {
	background-image:url('styles:DarkStyle/misc/radiobutton-checked.png');
}
 QRadioButton::indicator:checked:pressed {
	background-image:url('styles:DarkStyle/misc/radiobutton.png');
}

ImprovedTabBarCloseButton#ServerTabCloseButton,
ImprovedTabBarCloseButton#ChatTabCloseButton {
	background-color: transparent;
}

ImprovedTabBarCloseButton#ServerTabCloseButton:hover,
ImprovedTabBarCloseButton#ChatTabCloseButton:hover {
	background-color: rgba(130,130,130,0.1);
}

ImprovedTabBarCloseButton#ServerTabCloseButton:pressed,
ImprovedTabBarCloseButton#ChatTabCloseButton:pressed {
	background-color: rgba(130,130,130,0.2);
}


